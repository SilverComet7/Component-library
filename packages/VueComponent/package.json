{
  "name": "gmtool-vue-components",
  "version": "1.0.1",
  "description": "a common  vue component build , ignore vue version ",
  "main": "lib/v3",
  "scripts": {
    "build": "npm run build3 && npm run build2",
    "build2": "npx vue-demi-switch 2 && vite build --config build/v2/vite.config.ts",
    "build3": "npx vue-demi-switch 3 && vite build --config build/v3/vite.config.ts"
  },
  "type": "module",
  "keywords": [
    "vue2",
    "vue3",
    "vue-demi"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vue": "^3.0.0",
    "@vueuse/core": "^8.9.4",
    "element-plus": "2.2.10",
    "xlsx": "latest"
  },
  "devDependencies": {
    "vue-demi": "latest",
    "vite": "^3.0.3",
    "@vitejs/plugin-vue": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "@vue/composition-api": {
      "optional": true
    }
  }
}
